{
	"$schema": "https://biomejs.dev/schemas/2.3.11/schema.json",
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"a11y": {
				"noInteractiveElementToNoninteractiveRole": "off",
				"noNoninteractiveElementToInteractiveRole": "off",
				"noStaticElementInteractions": "off",
				"noSvgWithoutTitle": "off"
			}
		}
	},
	"formatter": {
		"enabled": true,
		"indentStyle": "tab"
	},
	"assist": {
		"actions": {
			"source": {
				"organizeImports": {
					"level": "on"
				}
			}
		}
	}
}
